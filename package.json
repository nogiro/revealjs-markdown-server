    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "puppeteer": "^1.17.0",
    "reveal.js": "^3.8.0",
    "yaml": "^1.5.1"
  },
  "devDependencies": {
    "@types/bytes": "^3.0.0",
    "@types/css": "0.0.31",
    "@types/express": "^4.16.1",
    "@types/node": "^12.0.0",
    "@types/puppeteer": "^1.12.4",
    "@types/yaml": "^1.0.2",
    "elm": "^0.19.0-no-deps",
    "node-elm-compiler": "^5.0.3",
