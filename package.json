    "typescript": "^3.4.5"
  },
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-index",
    "build-server": "parcel build --target node src/app.ts",
    "build-index": "parcel build src/front/elm-dummy.html && ls -t dist/front.* | sed '1,2d;s_.*_rm &_e'",
    "start": "node dist/app.js",
    "type-check": "tsc --noEmit --resolveJsonModule"
  }
}
